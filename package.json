{
  "name": "ng-keyboard-shortcuts-app",
  "version": "7.1.17",
  "description": "Dead Simple Keyboard Shortcuts Management for Angular 2+",
  "keywords": [
    "angular",
    "keyboard",
    "shortcuts",
    "keyboard-shortcuts",
    "rxjs"
  ],
  "license": "MIT",
  "author": "Omri Katz <omrikatz12@gmail.com> (https://github.com/omridevk)",
  "homepage": "https://omridevk.github.io/ng-keyboard-shortcuts/",
  "repository": "github.com/omridevk/ng-keyboard-shortcuts",
  "bugs": "https://github.com/omridevk/ng-keyboard-shortcuts/issues",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "clean": "rm -rf dist/",
    "serve": "npm-run-all --parallel start build:watch",
    "npm-pack": "cd dist/ng-keyboard-shortcuts && npm pack",
    "compodoc": "npx compodoc -p projects/ng-keyboard-shortcuts/tsconfig.lib.json --disableLifeCycleHooks --hideGenerator -d ./docs --disablePrivate",
    "compodoc-serve": "npx compodoc -p projects/ng-keyboard-shortcuts/tsconfig.lib.json --disableLifeCycleHooks --hideGenerator -s -d ./docs --disablePrivate -w",
    "build:prod": "npm run clean && npm run copy && ng build --prod ng-keyboard-shortcuts",
    "copy": "node ./scripts/copy.js",
    "build:watch": "npm run clean && ng build ng-keyboard-shortcuts --watch",
    "publish": "npm run build:prod && npm publish dist/ng-keyboard-shortcuts",
    "package": "npm run build:prod && npm run npm-pack",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/core": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/common": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/animations": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/compiler": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/forms": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/platform-browser": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/router": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "tslib": "^1.10.0",
    "@angular/cdk": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/material": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "core-js": "^3.6.0",
    "prettier": "^1.19.1",
    "rxjs": "^6.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.0",
    "@angular-devkit/build-ng-packagr": "^0.10.0",
    "@angular/cli": "~7.3.10",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.12.21",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-node": "~8.5.4",
    "tsickle": "^0.37.1",
    "tslib": "^1.10.0",
    "tslint": "~5.20.1",
    "typescript": "~3.1.1"
  }
}
